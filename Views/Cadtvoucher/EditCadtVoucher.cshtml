@using System.Data;
<script type="text/javascript">
    $(document).ready(function (e) {
        alert("Tq");
        $('.close').click(function (e) {
            $('#popup').hide();
        });
       
        $('.edit').click(function (e) {
            e.preventDefault();
            var myurl = $(this).attr('href');
            alert(myurl)
            $.ajax({
                type: 'POST',
                url: myurl,
                success: function (res) {
                    //alert(res);
                    $('.modal-dialog').html(res);
                    $('#popup').show();
                },
                error: function (error) {
                    $('.modal-dialog').html(error);
                    $('#popup').show();
                }
            });
        });

      
</script>



  @{DataRow dr=Model.Tables[0].Rows[0];}
<div class="content">
  <div class="header">
    <button type="button" class="close" data-dismiss="modal">&times;</button>
      @ViewBag.DialogTitle
  </div>
  <div class="modal-body">
	<div id="message" class="row"></div>
    <form id="EditCadtVoucher" name="EditCadtVoucher" role="form" action ="Voucher_masterEdit" >
      <fieldset id="form_set">
        		<div class="form-group" style="height:5px;display:none;">
			<label class="control-label col-xs-3 text-right">divid</label>
        	<input type="text" id="divid" name="divid" value="@dr["divid"]" />
		</div>
		<div class="form-group">
			<label class="control-label col-xs-3 text-right">divname</label>
			<input type="text" id="divname" name="divname" placeholder="divname" value="@dr["divname"]" />
		</div>
		
        <input type="submit" id="btndiv_master" class="btn btn-lg btn-success" value="Submit" />
      </fieldset>
    </form>
  </div>

</div>